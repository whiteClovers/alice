	$.ajax({
			type:"post" ,
			/* contentType:'application/x-www-form-urlencoded; charset=UTF-8', */
			url:'save.do',
			data:$('#formEdit').serialize(), //{"customerId":document.getElemenetById('customerId').value , }
			success:function(data){
				if(data == '操作成功'){
					window.location.href = 'page.do' ;
				}else{
					alert(data) ;	
				}
				
			}
		});
}
